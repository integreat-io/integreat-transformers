  },
  "homepage": "https://github.com/integreat-io/integreat-transformers#readme",
  "dependencies": {
    "luxon": "^3.0.3",
    "map-any": "^0.2.1",
    "map-transform": "^0.4.0-alpha.31"
  },
  "devDependencies": {
    "@integreat/ts-dev-setup": "^2.3.1",
    "integreat": "0.8.0-alpha.99"
  }
}
